{
	"name": "marttiphpbb/customcode",
	"type": "phpbb-extension",
	"description": "Custom Code Extension for phpBB 3.1",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-customcode",
	"version": "1.1.0",
	"time": "2014-11-15",
	"keywords": ["phpbb", "extension", "template", "custom"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "martti",
			"email": "info@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Custom Code",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3,<3.2.*@dev"
		},
		"version-check": {
			"host": "marttiphpbb.github.io",
			"directory": "/version",
			"filename": "customcode.json"
		}
	}
}
